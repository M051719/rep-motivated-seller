################################################################################
# ‚öñÔ∏è COMPLIANCE & LEGAL - REQUIRED FOR TCPA/GDPR/GLBA
################################################################################
# SMS/TCPA Compliance (Telephone Consumer Protection Act)
ENABLE_SMS_COMPLIANCE=true
TCPA_CONSENT_REQUIRED=true
SMS_OPT_IN_CONFIRMATION=true
SMS_OPT_OUT_KEYWORDS=STOP,STOPALL,UNSUBSCRIBE,CANCEL,END,QUIT
SMS_OPT_IN_KEYWORDS=START,YES,UNSTOP
SMS_HELP_KEYWORDS=HELP,INFO

# Default Compliance Messages
SMS_OPT_OUT_MESSAGE="You have been unsubscribed from RepMotivatedSeller SMS messages. Reply START to resubscribe. For help, reply HELP."
SMS_OPT_IN_MESSAGE="Welcome to RepMotivatedSeller! You'll receive important updates about foreclosure assistance. Reply STOP to unsubscribe. Msg&data rates may apply."
SMS_HELP_MESSAGE="RepMotivatedSeller foreclosure assistance. Text STOP to unsubscribe, START to resubscribe. Questions? Call (877) 806-4677. Msg&data rates may apply."

# GDPR Compliance (General Data Protection Regulation)
ENABLE_GDPR_COMPLIANCE=true
DATA_RETENTION_DAYS=2555  # 7 years for financial records (GLBA requirement)
PII_ENCRYPTION_ENABLED=true
COOKIE_CONSENT_REQUIRED=true

# Privacy & Data Protection
PRIVACY_POLICY_URL=https://repmotivatedseller.shoprealestatespace.org/privacy
TERMS_OF_SERVICE_URL=https://repmotivatedseller.shoprealestatespace.org/terms
DATA_PROTECTION_OFFICER_EMAIL=privacy@repmotivatedseller.shoprealestatespace.org

# GLBA Compliance (Gramm-Leach-Bliley Act) - Financial Data Protection
ENABLE_GLBA_COMPLIANCE=true
ENCRYPTION_AT_REST=true
ENCRYPTION_IN_TRANSIT=true
AUDIT_LOGGING_ENABLED=true

# Consent Tracking
CONSENT_TRACKING_ENABLED=true
CONSENT_VERSION=2026.01  # Update when T&C change
REQUIRE_EXPLICIT_CONSENT=true

# Legal Disclaimers
LEGAL_NOTICE_REQUIRED=true
LEGAL_NOTICE_VERSION=1.0
SHOW_LEGAL_BANNER=true
LEGAL_MODAL_ON_FIRST_VISIT=true

################################################################################
# üîß ENVIRONMENT CONFIGURATION
################################################################################
NODE_ENV=production
VITE_APP_ENV=production
PORT=3000

################################################################################
# FEATURE FLAGS - OPTIONAL
################################################################################
ENABLE_ANALYTICS=true
ENABLE_ERROR_REPORTING=true
ENABLE_DEBUG_MODE=false

