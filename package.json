{
  "name": "rep-motivated-seller",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Complete foreclosure assistance platform with AI support and education",
  "keywords": [
    "foreclosure",
    "real-estate",
    "education",
    "ai-assistant"
  ],
  "author": "RepMotivatedSeller Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "serve": "serve -s dist",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:connection": "node -e \"import('./src/utils/diagnostics.js').then(d => d.runEnhancedDiagnostics())\"",
    "health-check": "node -e \"import('./src/utils/diagnostics.js').then(d => d.quickHealthCheck().then(console.log))\"",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:deploy": "supabase functions deploy",
    "supabase:migrations": "supabase db push",
    "supabase:types": "supabase gen types typescript --local > src/types/database.types.ts",
    "supabase:seed": "supabase db seed",
    "supabase:reset": "supabase db reset",
    "db:setup": "supabase db execute --file setup_database.sql",
    "db:test": "supabase db execute --file test_database.sql",
    "db:inspect": "supabase db inspect",
    "db:status": "echo 'SELECT current_database(), current_user, version();' | supabase db execute",
    "db:migrate": "supabase db push",
    "db:backup": "supabase db dump --file backup_$(date +%Y%m%d_%H%M%S).sql",
    "policy:health": "bash scripts/policy-manager.sh health",
    "policy:audit": "bash scripts/policy-manager.sh audit",
    "policy:consolidate-dry": "bash scripts/policy-manager.sh consolidate-dry",
    "policy:fix-dry": "bash scripts/policy-manager.sh fix-dry",
    "policy:consolidate": "bash scripts/policy-manager.sh consolidate",
    "policy:fix": "bash scripts/policy-manager.sh fix",
    "start": "npm run serve",
    "deploy": "npm run build && npm run supabase:deploy",
    "build:production": "cross-env NODE_ENV=production vite build",
    "build:staging": "cross-env NODE_ENV=staging vite build",
    "analyze": "cross-env ANALYZE=true vite build",
    "analyze:open": "npm run analyze",
    "dev:network": "vite --host 0.0.0.0",
    "dev:debug": "cross-env DEBUG=true vite",
    "test:build": "tsc && vite build --mode test"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^5.3.0",
    "@stripe/stripe-js": "^8.5.1",
    "@supabase/storage-js": "^2.84.0",
    "@supabase/supabase-js": "^2.45.0",
    "framer-motion": "^10.16.5",
    "lucide-react": "^0.554.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.20.1",
    "tailwindcss": "^3.3.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^10.1.0",
    "esbuild": "^0.27.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "serve": "^14.2.1",
    "supabase": "^1.123.4",
    "terser": "^5.44.1",
    "vite": "^7.2.4",
    "vitest": "^4.0.13"
  },
  "peerDependencies": {
    "typescript": "^5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
