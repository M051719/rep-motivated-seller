{
  "name": "rep-motivated-seller",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Complete foreclosure assistance platform with AI support and education",
  "keywords": [
    "foreclosure",
    "real-estate",
    "education",
    "ai-assistant"
  ],
  "author": "RepMotivatedSeller Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:secure": "dotenvx run -- vite",
    "dev:encrypted": "dotenvx run -f .env -- vite",
    "build": "vite build",
    "build:production": "dotenvx run -f .env.production -- vite build",
    "build:staging": "dotenvx run -f .env.staging -- vite build",
    "build:typecheck": "tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 1000",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "preview:production": "dotenvx run -f .env.production -- vite preview",
    "preview:staging": "dotenvx run -f .env.staging -- vite preview",
    "serve": "serve -s dist",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest run src/tests/integration",
    "test:connection": "node -e \"import('./src/utils/diagnostics.js').then(d => d.runEnhancedDiagnostics())\"",
    "health-check": "node -e \"import('./src/utils/diagnostics.js').then(d => d.quickHealthCheck().then(console.log))\"",
    "snyk:test": "snyk test",
    "snyk:monitor": "snyk monitor",
    "snyk:protect": "snyk protect",
    "snyk:code": "snyk code test",
    "security:scan": "npm run snyk:test && npm run snyk:code",
    "security:hawk": "hawk scan",
    "security:hawk:quick": "hawk scan --config-override scanner.maxDuration=10",
    "security:hawk:full": "hawk scan --config-override scanner.maxDuration=60",
    "security:hawk:api": "hawk scan --config-override spider.enabled=false --api-scan-only",
    "security:report": "hawk scan --format json,html",
    "security:baseline": "hawk scan --create-baseline",
    "env:encrypt": "dotenvx encrypt",
    "env:encrypt:production": "dotenvx encrypt -f .env.production",
    "env:encrypt:staging": "dotenvx encrypt -f .env.staging",
    "env:encrypt:all": "dotenvx encrypt && dotenvx encrypt -f .env.production && dotenvx encrypt -f .env.staging",
    "env:decrypt": "dotenvx decrypt",
    "env:decrypt:production": "dotenvx decrypt -f .env.production",
    "env:get": "dotenvx get",
    "env:set": "dotenvx set",
    "env:keypair": "dotenvx keypair",
    "env:rotate": "dotenvx rotate",
    "env:ls": "dotenvx ls",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:deploy": "supabase functions deploy",
    "supabase:migrations": "supabase db push",
    "supabase:types": "supabase gen types typescript --local > src/types/database.types.ts",
    "supabase:seed": "supabase db seed",
    "supabase:reset": "supabase db reset",
    "db:setup": "supabase db execute --file setup_database.sql",
    "db:test": "supabase db execute --file setup_database.sql",
    "db:inspect": "supabase db inspect",
    "db:status": "echo 'SELECT current_database(), current_user, version();' | supabase db execute",
    "db:migrate": "supabase db push",
    "db:backup": "supabase db dump --file backup_$(date +%Y%m%d_%H%M%S).sql",
    "policy:health": "bash scripts/policy-manager.sh health",
    "policy:audit": "bash scripts/policy-manager.sh audit",
    "policy:consolidate-dry": "bash scripts/policy-manager.sh consolidate-dry",
    "policy:fix-dry": "bash scripts/policy-manager.sh fix-dry",
    "policy:consolidate": "bash scripts/policy-manager.sh consolidate",
    "policy:fix": "bash scripts/policy-manager.sh fix",
    "start": "npm run serve",
    "deploy": "npm run build && npm run supabase:deploy",
    "analyze": "cross-env ANALYZE=true vite build",
    "analyze:open": "npm run analyze",
    "dev:network": "vite --host 0.0.0.0",
    "dev:debug": "cross-env DEBUG=true vite",
    "test:build": "tsc && vite build --mode test"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.2.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/instrumentation-fetch": "^0.208.0",
    "@opentelemetry/instrumentation-xml-http-request": "^0.208.0",
    "@opentelemetry/otlp-exporter-base": "^0.208.0",
    "@opentelemetry/otlp-transformer": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-web": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/react-paypal-js": "^8.9.2",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.0",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/auth-helpers-react": "^0.15.0",
    "@supabase/storage-js": "^2.89.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.16",
    "@types/mapbox-gl": "^3.4.1",
    "chart.js": "^4.5.1",
    "drizzle-orm": "^0.45.1",
    "exceljs": "^4.4.0",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.23.26",
    "html2canvas": "^1.4.1",
    "ioredis": "^5.8.2",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.562.0",
    "mapbox-gl": "^3.17.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.8",
    "pptxgenjs": "^4.0.1",
    "react": "18.3.0",
    "react-calendly": "^4.4.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "18.3.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.69.0",
    "react-hot-toast": "^2.6.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.11.0",
    "stripe": "^20.1.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.8",
    "esbuild": "^0.27.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "serve": "^14.2.5",
    "snyk": "^1.1301.2",
    "supabase": "^2.70.5",
    "tailwindcss": "^3.3.6",
    "terser": "^5.44.1",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "typescript": "^5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
