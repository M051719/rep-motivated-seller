# Calendly Webhook Environment (example)
# Copy to .env.calendly or merge into your active env file.

# Required signing secret from Calendly Webhook subscription settings.
CALENDLY_WEBHOOK_SECRET=your_calendly_webhook_signing_key

# Optional: API access for follow-up actions (invitee details, cancellations, etc.)
VITE_CALENDLY_API_KEY=your_calendly_api_key
VITE_CALENDLY_ACCESS_TOKEN=your_personal_access_token
VITE_CALENDLY_USERNAME=repmotivatedseller
VITE_CALENDLY_USER_URI=https://api.calendly.com/users/your-user-id

# Event URLs (examples)
VITE_CALENDLY_FORECLOSURE_URL=repmotivatedseller/foreclosure-prevention-consultation
VITE_CALENDLY_BUSINESS_URL=repmotivatedseller/business-consultation
VITE_CALENDLY_GENERAL_URL=repmotivatedseller/general-consultation
